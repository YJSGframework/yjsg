<?xml version="1.0" encoding="utf-8"?>
<form>
	<fields name="attribs" addfieldpath="/plugins/system/yjsg/includes/yjsgmicrodata/elements">
		<fieldset name="YJSG_MICRODATA" label="YJSG_MICRODATA_ARTICLE_LABEL">
			<field name="yjsgmdatahandler" type="yjsgmdatahandler" />
			<field name="yjsg_microdata_enabeled" type="list" default="0"  class="yjsg-md-enabled" label="YJSG_MICRODATA_ARTICLE_ATIVATE_LABEL" description="YJSG_MICRODATA_ARTICLE_ATIVATE_DESC">
				<option value="0">YJSG_NO</option>
				<option value="1">YJSG_YES</option>
			</field>
			
			<field name="yjsg_microdata_position" type="list" default="0"  class="yjsg-md-position" label="YJSG_MICRODATA_ARTICLE_POSITION_LABEL" description="YJSG_MICRODATA_ARTICLE_POSITION_DESC">
				<option value="0">YJSG_MICRODATA_ARTICLE_POSITION_OPTION_ARTICLEHEADER</option>
				<option value="1">YJSG_MICRODATA_ARTICLE_POSITION_OPTION_AFTERARTICLETEXT</option>
			</field>
			
			<field name="yjsg_microdata_ratingtype" type="list" default="0"  class="yjsg-md-ratingtype" label="YJSG_MICRODATA_ARTICLE_RATINGTYPE_LABEL" description="YJSG_MICRODATA_ARTICLE_RATINGTYPE_DESC">
				<option value="none">YJSG_MICRODATA_ARTICLE_RATINGTYPE_OPTION_DONTUSE</option>
				<option value="ratingCount">YJSG_MICRODATA_ARTICLE_RATINGTYPE_OPTION_RATINGCOUNT</option>
				<option value="reviewCount">YJSG_MICRODATA_ARTICLE_RATINGTYPE_OPTION_REVIEWCOUNT</option>
			</field>
						
			<field name="yjsg_article_microdata" type="list" default="article"  class="yjsg-md-types" label="YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_LABEL" description="YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_DESC">
				<option value="Article">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_ARTICLE</option>
				<option value="BlogPosting">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_BLOGPOSTING</option>
				<option value="NewsArticle">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_NEWSARTICLE</option>
				<option value="MedicalScholarlyArticle">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_MEDICALSCHOLARLYARTICLE</option>
				<option value="TechArticle">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_TECHARTICLE</option>
				<option value="VideoObject">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_VIDEO</option>
				<option value="Event">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_EVENT</option>
				<option value="Product">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_PRODUCT</option>
				<option value="Recipe">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_RECIPE</option>
				<option value="Movie">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_MOVIE</option>
				<option value="Book">YJSG_MICRODATA_ARTICLE_RICHSNIPPETTYPE_OPTION_BOOK</option>
			</field>
			
			<!-- video vars -->
			
			<field name="yjsg_md_video_link" type="text" default=""  class="yjsg-md-video yjsg-md"  label="YJSG_MICRODATA_ARTICLE_VIDEO_LINK_LABEL" description="YJSG_MICRODATA_ARTICLE_VIDEO_LINK_DESC" />
			<field name="yjsg_md_video_thumb" type="text" default=""  class="yjsg-md-video yjsg-md"  label="YJSG_MICRODATA_ARTICLE_VIDEO_THUMBNAIL_LABEL" description="YJSG_MICRODATA_ARTICLE_VIDEO_THUMBNAIL_DESC" />
			<field name="yjsg_md_video_duration" type="text" default=""  class="yjsg-md-video yjsg-md"  label="YJSG_MICRODATA_ARTICLE_VIDEO_DURATION_LABEL" description="YJSG_MICRODATA_ARTICLE_VIDEO_DURATION_DESC" />
			
			<!-- event vars -->
			<field name="yjsg_md_event_start" type="calendar" format="%Y-%m-%d %H:%M:%S" default="" class="yjsg-md-event yjsg-md" label="YJSG_MICRODATA_ARTICLE_EVENT_START_LABEL" description="YJSG_MICRODATA_ARTICLE_EVENT_START_DESC" />
			<field name="yjsg_md_event_end" type="calendar" format="%Y-%m-%d %H:%M:%S" default="" class="yjsg-md-event yjsg-md"  label="YJSG_MICRODATA_ARTICLE_EVENT_END_LABEL" description="YJSG_MICRODATA_ARTICLE_EVENT_END_DESC" />
			<field name="yjsg_md_event_streetadress" type="text" default=""  class="yjsg-md-event yjsg-md"  label="YJSG_MICRODATA_ARTICLE_EVENT_STREET_LABEL" description="YJSG_MICRODATA_ARTICLE_EVENT_STREET_DESC" />
			<field name="yjsg_md_event_adresslocality" type="text" default=""  class="yjsg-md-event yjsg-md"  label="YJSG_MICRODATA_ARTICLE_EVENT_CITY_LABEL" description="YJSG_MICRODATA_ARTICLE_EVENT_CITY_DESC" />
			<field name="yjsg_md_event_adressregion" type="text" default=""  class="yjsg-md-event yjsg-md"  label="YJSG_MICRODATA_ARTICLE_EVENT_STATE_LABEL" description="YJSG_MICRODATA_ARTICLE_EVENT_STATE_DESC" />
			<field name="yjsg_md_event_postalcode" type="text" default=""  class="yjsg-md-event yjsg-md"  label="YJSG_MICRODATA_ARTICLE_EVENT_ZIPCODE_LABEL" description="YJSG_MICRODATA_ARTICLE_EVENT_ZIPCODE_DESC" />
			<field name="yjsg_md_event_price" type="text" default=""  class="yjsg-md-event yjsg-md"  label="YJSG_MICRODATA_ARTICLE_EVENT_PRICE_LABEL" description="YJSG_MICRODATA_ARTICLE_EVENT_PRICE_DESC" />
			<field name="yjsg_md_event_tickets" type="text" default=""  class="yjsg-md-event yjsg-md"  label="YJSG_MICRODATA_ARTICLE_EVENT_PURCHASELINK_LABEL" description="YJSG_MICRODATA_ARTICLE_EVENT_PURCHASELINK_DESC" />
			
			<!-- product vars -->
			<field name="yjsg_md_product_price" type="text" default=""  class="yjsg-md-product yjsg-md"  label="YJSG_MICRODATA_ARTICLE_PRODUCT_PRICE_LABEL" description="YJSG_MICRODATA_ARTICLE_PRODUCT_PRICE_DESC" />
			<field name="yjsg_md_product_availability" type="list" default="0"  class="yjsg-md-product yjsg-md" label="YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_LABEL" description="YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_DESC">
				<option value="na">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_DONTDISPLAY</option>
				<option value="Discontinued">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_DISCONTINUED</option>
				<option value="InStock">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_INSTOCK</option>
				<option value="InStoreOnly">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_INSTOREONLY</option>
				<option value="LimitedAvailability">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_LIMITEDAVAILABILITY</option>
				<option value="OnlineOnly">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_ONLINEONLY</option>
				<option value="OutOfStock">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_OUTOFSTOCK</option>
				<option value="PreOrder">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_PREORDER</option>
				<option value="SoldOut">YJSG_MICRODATA_ARTICLE_PRODUCT_AVAILABILITY_OPTION_SOLDOUT</option>
			</field>
			<!-- recipe vars -->
			
			<field name="yjsg_md_recipe_preptime" type="text" default=""  class="yjsg-md-recipe yjsg-md"  label="YJSG_MICRODATA_ARTICLE_RECIPE_PREPTIME_LABEL" description="YJSG_MICRODATA_ARTICLE_RECIPE_PREPTIME_DESC" />
			<field name="yjsg_md_recipe_cooktime" type="text" default=""  class="yjsg-md-recipe yjsg-md"  label="YJSG_MICRODATA_ARTICLE_RECIPE_COOKTIME_LABEL" description="YJSG_MICRODATA_ARTICLE_RECIPE_COOKTIME_DESC" />
			<field name="yjsg_md_recipe_totaltime" type="text" default=""  class="yjsg-md-recipe yjsg-md"  label="YJSG_MICRODATA_ARTICLE_RECIPE_TOTALTIME_LABEL" description="YJSG_MICRODATA_ARTICLE_RECIPE_TOTALTIME_DESC" />
			<field name="yjsg_md_recipe_ingredients" type="textarea" default="" rows="8" cols="40"  class="yjsg-md-recipe yjsg-md"  label="YJSG_MICRODATA_ARTICLE_RECIPE_INGREDIENTS_LABEL" description="YJSG_MICRODATA_ARTICLE_RECIPE_INGREDIENTS_DESC" />
			
			<!-- movie vars -->
			
			<field name="yjsg_md_movie_directors" type="textarea" default="" rows="8" cols="40" class="yjsg-md-movie yjsg-md"  label="YJSG_MICRODATA_ARTICLE_MOVIE_DIRECTORS_LABEL" description="YJSG_MICRODATA_ARTICLE_MOVIE_DIRECTORS_DESC" />
			<field name="yjsg_md_movie_writters" type="textarea" default="" rows="8" cols="40" class="yjsg-md-movie yjsg-md"  label="YJSG_MICRODATA_ARTICLE_MOVIE_WRITTERS_LABEL" description="YJSG_MICRODATA_ARTICLE_MOVIE_WRITTERS_DESC" />
			<field name="yjsg_md_movie_stars" type="textarea" default="" rows="8" cols="40" class="yjsg-md-movie yjsg-md"  label="YJSG_MICRODATA_ARTICLE_MOVIE_STARS_LABEL" description="YJSG_MICRODATA_ARTICLE_MOVIE_STARS_DESC" />
			
			<!-- book vars -->
			<field name="yjsg_md_book_price" type="text" default=""  class="yjsg-md-book yjsg-md"  label="YJSG_MICRODATA_ARTICLE_BOOK_PRICE_LABEL" description="YJSG_MICRODATA_ARTICLE_BOOK_PRICE_DESC" />
			<field name="yjsg_md_book_availability" type="list" default="0"  class="yjsg-md-book yjsg-md" label="YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_LABEL" description="YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_DESC">
				<option value="na">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_DONTDISPLAY</option>
				<option value="Discontinued">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_DISCONTINUED</option>
				<option value="InStock">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_INSTOCK</option>
				<option value="InStoreOnly">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_INSTOREONLY</option>
				<option value="LimitedAvailability">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_LIMITEDAVAILABILITY</option>
				<option value="OnlineOnly">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_ONLINEONLY</option>
				<option value="OutOfStock">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_OUTOFSTOCK</option>
				<option value="PreOrder">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_PREORDER</option>
				<option value="SoldOut">YJSG_MICRODATA_ARTICLE_BOOK_AVAILABILITY_OPTION_SOLDOUT</option>
			</field>
			<field name="yjsg_md_book_pages" type="text" default=""  class="yjsg-md-book yjsg-md"  label="YJSG_MICRODATA_ARTICLE_BOOK_PAGES_LABEL" description="YJSG_MICRODATA_ARTICLE_BOOK_PAGES_DESC" />
			<field name="yjsg_md_book_author" type="text" default=""  class="yjsg-md-book yjsg-md"  label="YJSG_MICRODATA_ARTICLE_BOOK_AUTHOR_LABEL" description="YJSG_MICRODATA_ARTICLE_BOOK_AUTHOR_DESC" />
			<field name="yjsg_md_book_publisher" type="text" default=""  class="yjsg-md-book yjsg-md"  label="YJSG_MICRODATA_ARTICLE_BOOK_PUBLISHER_LABEL" description="YJSG_MICRODATA_ARTICLE_BOOK_PUBLISHER_DESC" />
			<field name="yjsg_md_book_language" type="text" default=""  class="yjsg-md-book yjsg-md"  label="YJSG_MICRODATA_ARTICLE_BOOK_LANGUAGE_LABEL" description="YJSG_MICRODATA_ARTICLE_BOOK_LANGUAGE_DESC" />
		</fieldset>
	</fields>
</form>
